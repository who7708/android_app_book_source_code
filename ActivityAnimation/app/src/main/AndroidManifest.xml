<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ansen.activityanimation">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ansen.activityanimation.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.ansen.activityanimation.SecondActivity"/>
        <activity android:name="com.ansen.activityanimation.ThreeActivity"/>
        <activity android:name="com.ansen.activityanimation.ExplodeActivity"/>
        <activity android:name="com.ansen.activityanimation.SlideActivity"/>
        <activity android:name="com.ansen.activityanimation.FadeActivity"/>
        <activity android:name="com.ansen.activityanimation.SharedElementActivity"/>
        <activity android:name="com.ansen.activityanimation.SecondShareElemActivity"/>
        <activity android:name="com.ansen.activityanimation.CustomSharedElementActivity"/>
    </application>

</manifest>